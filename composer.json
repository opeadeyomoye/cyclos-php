{
    "name": "techequity/cyclos-php",
    "type": "library",
    "description": "PHP client library for Cyclos 4.8+",
    "keywords": ["cyclos","cyclos-4.8", "banking", "e-money"],
    "homepage": "https://github.com/opeadeyomoye/cyclos-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ope Adeyomoye",
            "email": "victoradeyomoye@yahoo.co.uk",
            "homepage": "https://twitter.com/victoradeyomoye",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "nategood/httpful": "*"
    },
    "autoload": {
        "psr-4": {
            "Cyclos\\": "src/Cyclos/",
            "Cyclos\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "vlucas/phpdotenv": "Needed to use Cyclos API access config stored in your local .env file."
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
